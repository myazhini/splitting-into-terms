import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int d1 = sc.nextInt(), c1 = sc.nextInt();
        int d2 = sc.nextInt(), c2 = sc.nextInt();

        int totalCents = c1 + c2;
        int totalDollars = d1 + d2 + totalCents / 100;
        totalCents %= 100;

        System.out.println(totalDollars);
        System.out.println(totalCents);
    }
}
